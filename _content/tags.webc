<h1>Tags</h1>
<ul>
  <!--tag list-->
  <script webc:type="js">
    filterTagList(Object.keys(collections))
      .map((tag) => {
        tagURL = `/tags/${slugify(tag)}/`;
        return `<li><a href="${tagURL}" class="post-tag">${tag}</a></li>`;
      })
      .join("\n");
  </script>
  <!--/tag list-->
</ul>
