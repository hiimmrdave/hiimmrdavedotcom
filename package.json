{
  "name": "mr-dave-blog",
  "version": "1.0.0",
  "description": "Mr. Dave's blog, based on eleventy-base-blog",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "build-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
    "start": "npx @11ty/eleventy --serve --quiet",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
    "debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
    "benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hiimmrdave/hiimmrdavedotcom.git"
  },
  "author": {
    "name": "Dave Vogt",
    "url": "https://hiimmrdave.com/"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.3",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@11ty/eleventy-plugin-webc": "^0.9.3",
    "@kevingimbel/eleventy-plugin-mermaid": "^2.0.2",
    "eleventy-plugin-emoji": "^1.1.0",
    "luxon": "^3.2.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.6.6",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.3",
    "stylelint": "^15.1.0",
    "stylelint-config-standard": "^30.0.1"
  }
}
